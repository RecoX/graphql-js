type Link {
  id: ID! @unique
  createdAt: DateTime!
  description: String!
  url: String!
  postedBy: User
  votes: [Vote!]!
}

type User {
  id: ID! @unique
  name: String!
  email: String! @unique
  password: String!
  links: [Link!]!
  votes: [Vote!]!
}

type Vote {
  id: ID! @unique
  link: Link!
  user: User!
}

type Image {
  id: ID! @unique
  name: String!
  createdAt: DateTime!
  description: String!
  uploadedBy: User
  origin: String
  votes: [VoteImage!]
  url: String
  #  indexingData: IndexingData!
}

type VoteImage {
  id: ID! @unique
  image: Image!
  user: User!
}
